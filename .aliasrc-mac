alias ls='ls -G'
alias grep='grep -G'
alias curl='/usr/bin/curl'
alias vim='/usr/local/bin/mvim'
alias inspect_hiera='python2.7 ~/bin/inspect_hiera'
alias arsenal='arsenal --conf ~/app/arsenal/conf/arsenal.ini'
alias arsenalinf1='arsenal --conf ~/app/arsenal/conf/arsenal-inf1.ini'
alias awsrd='aws --profile rubicon-dev'
alias awsrq='aws --profile rubicon-qa'
alias awsrp='aws --profile rubicon-prod'
alias awsAccessDev='docker run -it -v ~/.onelogin:/root/.onelogin -v ~/.aws:/root/.aws docker.rp-core.com/rubiconproject/onelogin-python-aws-assume-role:6_master onelogin-aws-assume-role --profile trp-rubicon-dev'
alias awsAccessQa='docker run -it -v ~/.onelogin:/root/.onelogin -v ~/.aws:/root/.aws docker.rp-core.com/rubiconproject/onelogin-python-aws-assume-role:6_master onelogin-aws-assume-role --profile trp-rubicon-qa'
alias awsAccessProd='docker run -it -v ~/.onelogin:/root/.onelogin -v ~/.aws:/root/.aws docker.rp-core.com/rubiconproject/onelogin-python-aws-assume-role:6_master onelogin-aws-assume-role --profile trp-rubicon-prod'
